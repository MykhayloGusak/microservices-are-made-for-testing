{
  "name": "@applitools/microservices-were-made-for-testing",
  "version": "1.0.0",
  "description": "",
  "main": "src/microservices-were-made-for-testing.js",
  "engines": {
    "node": ">=10.3.0"
  },
  "files": [
    "helm",
    "src",
    "scripts"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "bin": {
    "run-microservices-were-made-for-testing": "scripts/run-microservices-were-made-for-testing.js"
  },
  "scripts": {
    "start": "scripts/run-microservices-were-made-for-testing.js",
    "test": "npm run eslint && npm run test:mocha-parallel",
    "test:mocha": "mocha --no-timeouts --trace-warnings --exit 'test/unit/*.test.js' 'test/it/*.test.js' 'test/e2e/*.test.js'",
    "test:mocha-parallel": "mocha-parallel-tests --no-timeouts --trace-warnings --exit 'test/unit/*.test.js' 'test/it/*.test.js' 'test/e2e/*.test.js'",
    "eslint": "eslint '**/*.js'",
    "build": "#",
    "postbuild": "npm run build:docker",
    "build:docker": "docker build -t applitools/microservices-were-made-for-testing --build-arg NPM_FILE=`cat ~/.npmrc` .",
    "postpublish": "npm run publish:docker",
    "publish:docker": "docker tag applitools/microservices-were-made-for-testing applitools/microservices-were-made-for-testing:${npm_package_version} && docker push applitools/microservices-were-made-for-testing:${npm_package_version} && docker push applitools/microservices-were-made-for-testing:latest",
    "deploy": "kdeploy deploy microservices-were-made-for-testing ${npm_package_version} -c worker -v ./node_modules/@applitools/rendering-grid-deployment ."
  },
  "keywords": [
    "applitools",
    "testing",
    "browser",
    "grid",
    "visual-testing",
    "nodejs",
    "javascript"
  ],
  "author": "Gil Tayar <gil.tayar@applitools.com>",
  "license": "UNLICENSED",
  "repository": {
    "url": "https://github.com/applitools/rendering-grid",
    "type": "git"
  },
  "dependencies": {
    "debug": "^4.1.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@applitools/docker-compose-mocha": "^1.3.0",
    "@applitools/docker-compose-testkit": "^1.1.2",
    "@applitools/rendering-grid-deployment": "^3.0.23",
    "chai": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-plugin-mocha-no-only": "^1.0.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "mocha": "^5.2.0",
    "mocha-parallel-tests": "^2.2.1",
    "node-fetch": "^2.6.0",
    "prettier": "^1.18.2"
  }
}
